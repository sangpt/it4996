<!-- page content -->
<div class="right_col" role="main">
  <%= render "top_title" %>
  <div class="">
    <div class="clearfix"></div>
    <div class="row">
      <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="x_panel">
          <div class="x_title">
            <h2>Request Chart</h2>
            <div class="clearfix"></div>
          </div>
          <div class="x_content">
            <%= render "chart_number_requests", data: @data %>
          </div>
        </div>

        <div class="x_panel">
          <div class="x_title">
            <h2>All Request</h2>
            <div class="clearfix"></div>
          </div>
          <div class="x_content">
            <%= render "table_requests", data: @data %>
          </div>
        </div>
      </div>
    </div>

  </div>
</div>

<script type="text/javascript">
  <%= render "javascript.js" %>
</script>
<!-- /page content -->
