<table id="datatable" class="table table-striped table-bordered">
  <thead>
    <tr>
      <th class="stt">#</th>
      <th>Client name</th>
      <th>Time create</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Quantity</th>
      <th>Total</th>
      <th>Cash</th>
    </tr>
  </thead>
  <tbody>
    <% pay_histories.each do |record| %>
      <tr>
        <td></td>
        <td><%= record.client.name %></td>
        <td><%= record.created_at %></td>
        <td><%= record.start_date %></td>
        <td><%= record.end_date %></td>
        <td><%= record.quantity.to_i %></td>
        <td><%= record.total.to_i %>$</td>
        <td><%= record.cash.to_i %>$</td>
      </tr>
    <% end %>
  </tbody>
</table>
